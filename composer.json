{
  "name"       : "jdelgado/form-bundle",
  "type"       : "symfony-bundle",
  "description": "Symfony bundle for rendering a simple form",
  "keywords"   : ["Voting", "Rating"],
  "homepage"   : "https://github.com/nerioth11/FormBundle",
  "license"    : "MIT",
  "authors"    : [
    {
      "name" : "Joel Delgado",
      "email": "jdelgado@intechpartner.com"
    }
  ],
  "require": {
    "php"                     : "^7.1",
    "symfony/framework-bundle": "^4.0",
    "symfony/security-bundle" : "^4.0",
    "symfony/translation"     : "^4.0",
    "symfony/twig-bundle"     : "^4.0",
    "symfony/yaml"            : "^4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Jdelgado\\FormBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jdelgado\\FormBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "<4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable"    : true
}
